<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <filter>
    <filter-name>InputFilter</filter-name>
    <filter-class>se.dreamteam.atm.web.InputFilter</filter-class>
  </filter>
   <filter>
    <filter-name>SessionInputFilter</filter-name>
    <filter-class>se.dreamteam.atm.web.SessionInputFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>InputFilter</filter-name>
    <servlet-name>ATMServlet</servlet-name>
  </filter-mapping>
  <filter-mapping>
    <filter-name>SessionInputFilter</filter-name>
    <servlet-name>ATMSession</servlet-name>
  </filter-mapping>
  <servlet>
    <servlet-name>ATMServlet</servlet-name>
    <servlet-class>se.dreamteam.atm.web.ATMServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ATMSession</servlet-name>
    <servlet-class>se.dreamteam.atm.web.ATMSession</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ATMServlet</servlet-name>
    <url-pattern>/ATM</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ATMSession</servlet-name>
    <url-pattern>/ATMSession</url-pattern>
  </servlet-mapping>
</web-app>